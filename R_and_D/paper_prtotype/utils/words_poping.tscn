resource/name = "words_poping"
script/source = "#\t\t\t\t\twords_popping\nextends Node2D\n\nvar gravity = 0.08\nvar gravity_pull = 0\nvar init_velocity = Vector2(2,-15)\nvar steps = 1000\nvar ground_pos = get_pos().y\nvar mass = 0.5\nvar velocity = init_velocity\nvar wait = 0\nvar direction = 0 \nvar notepad_position = Vector2()\n\n\nvar timer = Timer.new()\n\n\nfunc _ready():\n\trandomize()\n\tinit_velocity.y = rand_range(-15.0,-20.0)\n\tground_pos += rand_range(-50.9,50.9)\n\t\n\twait = rand_range(0,70)\n\t\n\tset_process(true)\n\ttimer.set_one_shot(true)\n\ttimer.set_wait_time(0.005)\n\tadd_child(timer)\n\tword_pop()\n\tpass\n\t\nfunc word_pop():\n\t\n\tinit_velocity.x = rand_range(1.5,2.5) * direction\n\tvelocity = init_velocity\n\n\tset_pos(get_pos() + velocity)\n\twhile(steps != 0):\n\t\twait -= 1\n\t\ttimer.start()\n\t\tif(0 > wait):\n\t\t\t\n\t\t\tif(!(gravity_pull >= 1)): gravity_pull += gravity / mass\n\t\t\tvelocity.y += gravity_pull\n\t\t\tset_pos(get_pos() + velocity)\n\t\t\tif(get_pos().y >= ground_pos):\n\t\t\t\tvelocity = init_velocity\n\t\t\t\tinit_velocity *= 0.5\n\t\t\t\tgravity_pull = 0\n\t\t\t\tif(abs(velocity.length()) < 0.5): break\n\t\t\t\tset_pos(get_pos() + velocity)\n\t\n\t\tsteps -= 1\n\t\tyield(timer,\'timeout\')\n\twait = rand_range(20,60)\n\t\n\tdirection = Vector2()\n\twhile(steps != 0):\n\t\ttimer.start()\n\t\twait -= 1\n\t\tif(wait < 0):\n\t\t\tvar target = notepad_position\n\t\t\tvar glob_pos = get_global_pos()\n\t\t\tdirection = (target - glob_pos) * 0.2\n\t\t\tset_global_pos(glob_pos + (direction * 0.5))\n\t\t\tif(target - glob_pos < Vector2(0.5,0.5)): break\n\t\tsteps -= 1\n\t\tyield(timer,\'timeout\')\n\t\t\n\twait = 40\n\twhile(wait > 0):\n\t\ttimer.start()\n\t\twait -= 1\n\t\tyield(timer,\'timeout\')\n\t\tpass\n\tqueue_free()\n\t\t\n\n\t\nfunc change_word(_word):\n\tset_name(_word)\n\tget_node(\"word_label\").set_text(_word)\n\tpass\n\t\n\t\n\t\n\n"

