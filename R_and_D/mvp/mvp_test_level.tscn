[sub_resource type="GDScript" id=8]

resource/name = "ui_level_and_cash"
script/source = "#					ui_level_and_cash
extends Sprite

var ui_cash_label
var current_ui_cash
var game_cash
var stay_active_timer
var anim
var is_counting = false
var count_down_or_up_timer = Timer.new()

func _ready():
	
	add_child(count_down_or_up_timer)
	ui_cash_label		= get_node(\"cash_label\")
	stay_active_timer	= get_node(\"stay_active_timer\")
	anim				= get_node(\"anim\")
	
	stay_active_timer.connect(\"timeout\",self,\"fade_out\")
	


func update_cash(_game_cash):
	game_cash = _game_cash
	is_counting = true
	make_visable()
	
	count_down_or_up_timer.connect(\"timeout\",self,\"count_down_or_up\")
	count_down_or_up_timer.set_wait_time(0.015)
	
	count_down_or_up_timer.start()


func count_down_or_up():
	current_ui_cash = int(ui_cash_label.get_text())
	if(current_ui_cash 	< 	game_cash): current_ui_cash += 1
	if(current_ui_cash 	> 	game_cash): current_ui_cash -= 1
	ui_cash_label.set_text(str(current_ui_cash))
	
	if(current_ui_cash 	== 	game_cash): 
		count_down_or_up_timer.disconnect(\"timeout\",self,\"count_down_or_up\")
		stay_active_timer.start()
		is_counting = false
		return
	count_down_or_up_timer.start()
	

func make_visable():
	show()
	if(is_counting == false): stay_active_timer.start()
	anim.play(\"fade_in\")
	
func make_invisable()	: hide()
func fade_out()			: anim.play(\"fade_out\")
	

"

