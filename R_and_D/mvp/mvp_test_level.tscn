[sub_resource type="GDScript" id=8]

resource/name = "ui_level_and_cash"
script/source = "#\t\t\t\t\tui_level_and_cash\nextends Sprite\n\nvar ui_cash_label\nvar current_ui_cash\nvar game_cash\nvar stay_active_timer\nvar anim\nvar is_counting = false\nvar count_down_or_up_timer = Timer.new()\n\nfunc _ready():\n\t\n\tadd_child(count_down_or_up_timer)\n\tui_cash_label\t\t= get_node(\"cash_label\")\n\tstay_active_timer\t= get_node(\"stay_active_timer\")\n\tanim\t\t\t\t= get_node(\"anim\")\n\t\n\tstay_active_timer.connect(\"timeout\",self,\"fade_out\")\n\t\n\n\nfunc update_cash(_game_cash):\n\tgame_cash = _game_cash\n\tis_counting = true\n\tmake_visable()\n\t\n\tcount_down_or_up_timer.connect(\"timeout\",self,\"count_down_or_up\")\n\tcount_down_or_up_timer.set_wait_time(0.015)\n\t\n\tcount_down_or_up_timer.start()\n\n\nfunc count_down_or_up():\n\tcurrent_ui_cash = int(ui_cash_label.get_text())\n\tif(current_ui_cash \t< \tgame_cash): current_ui_cash += 1\n\tif(current_ui_cash \t> \tgame_cash): current_ui_cash -= 1\n\tui_cash_label.set_text(str(current_ui_cash))\n\t\n\tif(current_ui_cash \t== \tgame_cash): \n\t\tcount_down_or_up_timer.disconnect(\"timeout\",self,\"count_down_or_up\")\n\t\tstay_active_timer.start()\n\t\tis_counting = false\n\t\treturn\n\tcount_down_or_up_timer.start()\n\t\n\nfunc make_visable():\n\tshow()\n\tif(is_counting == false): stay_active_timer.start()\n\tanim.play(\"fade_in\")\n\t\nfunc make_invisable()\t: hide()\nfunc fade_out()\t\t\t: anim.play(\"fade_out\")\n\t\n\n"

