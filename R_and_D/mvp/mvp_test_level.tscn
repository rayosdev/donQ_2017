resource/name = "word_notefication"
script/source = "#					word_notefication
extends Sprite

var wait_timer
var wait_queue = 0


func _ready(): 
	wait_timer = get_node(\"wait_timer\")
	wait_timer.connect(\"timeout\",self,\"run_queue_step\")
#	_Words.connect(
	

var word_count = 0 setget set_word_count, get_word_count
func set_word_count(_word_count): word_count = _word_count
func get_word_count(): return word_count
func change_word_count(by_number): 
	word_count += by_number


func add_to_queue():
	wait_queue += 1
	wait_timer.start()
	show()


func visual_update():
	if(word_count > 0): show()
	else: 
		hide()
		return
	get_node(\"anim\").play(\"word_added\")
	get_node(\"label\").set_text(str(word_count))


func run_queue_step():
	if(self.is_visible() == false):
		wait_timer.start()
		return
	if(wait_queue > 0):
		wait_timer.start()
		wait_queue -= 1
		change_word_count(1)
		visual_update()
		
	pass"

